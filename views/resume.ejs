      <!-- #primary BEGIN -->
      <div id="represent" style="border-top: 8px solid #ff8a73;">
         <!-- .hentry BEGIN -->
         <div id="resume">
            <!-- .post-title BEGIN -->
            <div class="upper" style="background: #ff8a73;">
               <div class="inside">
                  <div class="wrapper">
                     <div class="profile-picture"><img alt='' src='<%= user.avatar %>' class='avatar avatar-512 photo' height='512' width='512' /></div>
                     <div class="brief">
                        <h1><%= user.displayName %></h1>
                        <h3><%= resume.title %></h3>
                        <p><%= resume.tagline %></p>
                     </div>
                  </div>
               </div>
            </div>
         </div>
         <!-- .post-entry BEGIN -->
         <div class="main">
            <div class="inside">
               <ul class="experience">
                  <div class="wrapper">
                     <h2>
                        <div class="experience-icon" style="background-color: #ff8a73;"></div>
                        Experience
                     </h2>
                     <div class="grouping">
                     	<br/>
                        <!-- <li>
                           <h3>Software Engineer Intern</h3>
                           <h4>Eventable</h4>
                           <p class="years">Summer 2013</p>
                           <div class="bio">
                              <p>Worked on analytics using Django platform. Redesigned frontend for subscription dialog.</p>
                           </div>
                        </li> -->
                        <% for (var i = 0; i < parseInt(resume.experience_count) + 1; i++) { %>
                           <% if (i == 0) { %>
	                        <img id="exp0" src="<%= resume.experience_0_imgurl %>" title="<%= resume.experience_0 %>" width="200" height="125"/>
	                       <% } %>
	                        <% if (i == 1) { %>
	                        <img id="exp1" src="<%= resume.experience_1_imgurl %>" title="<%= resume.experience_1 %>" width="200" height="125"/>
	                        
	                       <% } %>
	                        <% if (i == 2) { %>
	                        <img id="exp2" src="<%= resume.experience_2_imgurl %>" title="<%= resume.experience_2 %>" width="200" height="125"/>
	                        
	                       <% } %>
	                       <% if (i == 3) { %>
	                        <img id="exp3" src="<%= resume.experience_3_imgurl %>" title="<%= resume.experience_3 %>" width="200" height="125"/>
	                        
	                       <% } %>
	                       <% if (i == 4) { %>
	                        <img id="exp4" src="<%= resume.experience_4_imgurl %>" title="<%= resume.experience_4 %>" width="200" height="125"/>
	                        
	                       <% } %>
	                       <% if (i == 5) { %>
	                        <img id="exp5" src="<%= resume.experience_5_imgurl %>" title="<%= resume.experience_5 %>" width="200" height="125"/>
	                        
	                       <% } %>
	                    <% } %>
                        <div id="filler"></div>

                     </div>
                  </div>
               </ul>
               <ul class="education">
                  <div class="wrapper">
                     <h2>
                        <div class="education-icon" style="background-color: #ff8a73;"></div>
                        Education
                     </h2>
                     <div class="grouping">
                     	<br/>
                        <!-- <li>
                           <h3>Monta Vista High School</h3>
                           <p class="years">2010-2013</p>
                           <div class="bio">
                              <p>Completed AP Computer Science</p>
                           </div>
                        </li>
                        <li>
                           <h3>De Anza College</h3>
                           <p class="years">2013-2014</p>
                           <div class="bio">
                              <p>Part of the high school College Now program. Taking CIS series courses.</p>
                           </div>
                        </li> -->
                        <% for (var i = 0; i < parseInt(resume.education_count) + 1; i++) { %>
                           <% if (i == 0) { %>
	                        <img src="<%= resume.education_0_imgurl %>" title="<%= resume.education_0 %>" width="200" height="125" />
	                       <% } %>
	                        <% if (i == 1) { %>
	                        <img src="<%= resume.education_1_imgurl %>" title="<%= resume.education_1 %>" width="200" height="125" />
	                       <% } %>
	                        <% if (i == 2) { %>
	                        <img src="<%= resume.education_2_imgurl %>" title="<%= resume.education_2 %>" width="200" height="125" />
	                       <% } %>
	                       <% if (i == 3) { %>
	                        <img src="<%= resume.education_3_imgurl %>" title="<%= resume.education_3 %>" width="200" height="125" />
	                       <% } %>
	                       <% if (i == 4) { %>
	                        <img src="<%= resume.education_4_imgurl %>" title="<%= resume.education_4 %>" width="200" height="125" />
	                       <% } %>
	                       <% if (i == 5) { %>
	                        <img src="<%= resume.education_5_imgurl %>" title="<%= resume.education_5 %>" width="200" height="125" />
	                       <% } %>
	                    <% } %>
                     </div>
                  </div>
               </ul>
               <ul class="skills">
                  <div class="wrapper">
                     <h2>
                        <div class="skills-icon" style="background-color: #ff8a73;"></div>
                        Skills
                     </h2>
                     <div class="grouping">
                     	<% for (var i = 0; i < parseInt(resume.skill_count) + 1; i++) { %>
	                        <li>
	                           <% if (i == 0) { %>
	                           	<h4><%= resume.skill_0 %></h4>
	                           <% } %>
	                           <% if (i == 1) { %>
	                           	<h4><%= resume.skill_1 %></h4>
	                           <% } %>
	                           <% if (i == 2) { %>
	                           	<h4><%= resume.skill_2 %></h4>
	                           <% } %>
	                           <% if (i == 3) { %>
	                           	<h4><%= resume.skill_3 %></h4>
	                           <% } %>
	                           <% if (i == 4) { %>
	                           	<h4><%= resume.skill_4 %></h4>
	                           <% } %>
	                        </li>
	                    <% } %>
                     </div>
                  </div>
               </ul>
               <ul class="contact">
                  <div class="wrapper">
                     <h2>
                        <div class="contact-icon" style="background-color: #ff8a73;"></div>
                        Contact
                     </h2>
                     <div class="grouping">
                        <li>
                           <h4><a style="color:black;"><%= resume.contact %></a></h4>
                        </li>
                     </div>
                  </div>
               </ul>
            </div>
            <div id="footer">
               <div class="inside">
                  <p class="represent"><a href="#">Resonate your resume</a></p>
                  <p class="represent" style="float:right"><button type="button" style="color:black;background:none;border:none;" onClick="func()">Get PDF</button></p>
